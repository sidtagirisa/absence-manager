{
  "name": "absence-manager-server",
  "version": "1.0.0",
  "description": "Server for absence manager",
  "author": "Siddhardha Tagirisa",
  "scripts": {
    "start": "node -r @babel/register -r ./src/index.js",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "precover": "rm -rf coverage",
    "cover": "jest --coverage --collectCoverageFrom=src/**/*.js",
    "postcover": "cp coverage/coverage-final.json coverage/coverage.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test",
      "pre-push": "npm run cover"
    }
  },
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "ics": "^2.19.0",
    "moment": "^2.29.2",
    "moment-range": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "babel-core": "^6.25.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-watch": "^6.0.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "prettier": "^2.0.5"
  }
}
